<XML>
  <logging type="string">NONE</logging>
  <real-time-factor type="double">1</real-time-factor>
  
  <init-file type="file">@@PACER_MODEL_PATH@@/@@TESTING_ROBOT@@/init.xml</init-file>
  <robot-model type="string">@@PWD@@/model.xml</robot-model>
  <init>
    <base>
      <x type="double">0 0 0 0 0 0</x>
    </base>
  </init>
  <moby>
    <display-moby-skeleton type="bool">true</display-moby-skeleton>
  </moby>
  
  <task-file type="file">plugins.xml</task-file>

</XML>
