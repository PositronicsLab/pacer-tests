  <XML>
  <monte-carlo-@@PWD@@>
    <file type="string">monte-carlo-simulation/libmonte-carlo-simulation.so</file>
    <real-time-factor type="int">1</real-time-factor>
    <priority type="double">0</priority>
    <duration type="double">2.0</duration>
    <dt type="double">0.001</dt>
    <start-at-time type="double">0.1</start-at-time>
    <display type="bool">false</display>
    <visualize-step type="int">0</visualize-step>
    <output-model type="bool">false</output-model>
    <get-data-online type="bool">true</get-data-online>
    <max-threads type="int">10</max-threads>
    <max-samples type="int">10</max-samples>
    <executable type="string">monte-carlo-simulation/sample.bin</executable>
    <task-directory type="string vector">@@PWD@@</task-directory>
  </monte-carlo-@@PWD@@>
</XML>
