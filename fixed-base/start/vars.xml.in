<XML>
  <logging type="string">INFO</logging>
  <real-time-factor type="double">1</real-time-factor>
  
  <init-file type="file">@@PACER_MODEL_PATH@@/@@TESTING_ROBOT@@/init.xml</init-file>
  
  <moby>
    <wait-timestep type="bool">true</wait-timestep>
  </moby>
  <main>
    <realtime type="bool">true</realtime>
  </main>
  <init>
    <base>
      <x type="double"> 0 0 0 0 0 0 </x> 
      <xd type="double"> 0 0 0 0 0 0 </xd> 
    </base>
  </init>
  
  <task-file type="file">plugins.xml</task-file>
  <uncertainty-file type="file">@@PACER_MODEL_PATH@@/@@TESTING_ROBOT@@/uncertainty.xml</uncertainty-file>

</XML>
